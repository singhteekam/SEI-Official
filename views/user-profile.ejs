<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet"
    integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
    integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
  <link rel="icon" href="/logo.png">
  <link rel="stylesheet" href="/styles.css">
  <link rel="stylesheet" href="/bootstrap-social.css">
  <title>SEI</title>
</head>

<body onload="myFunc()">

  <div class="loader_bg">
    <div class="loader"></div>
  </div>

  <!-- ======================== Navbar ======================= -->

  <div id="home">
    <nav class="navbar navbar-expand-md navbar-dark">
      <a href="#"><img src="/logo.png" alt=""></a>

      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
        aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>

      <div class="collapse navbar-collapse text-center" id="navbarSupportedContent">
        <ul class="navbar-nav ml-auto">

          <li class="nav-item active">
            <a class="nav-link navhover text-white ml-3" href="/admin-panel1">Pending Forms <span
                class="sr-only">(current)</span></a>
          </li>

          <li class="nav-item active">
            <a class="nav-link navhover text-white ml-3" href="/admin-panel2">Registered Students</a>
          </li>
          <li class="nav-item active">
            <a class="nav-link navhover text-white ml-3" href="/changeAdminPassword">Change Password</a>
          </li>
          <li class="nav-item active">
            <a class="nav-link navhover text-white ml-3" href="/logout">Logout</a>
          </li>

      </div>
    </nav>
  </div>

<!-- ========== Section Begins ========== -->

<section class="sec container mb-5">
  <h2 class="card-title mt-5 text-center"><i class="fa fa-circle"></i> <%= info.firstName %>-Profile <i
      class="fa fa-circle"></i>
  </h2>

  <div class="card d-block m-auto">
    <img class="card-img-top d-block m-auto" style="border-radius: 100%; height: 15rem; width: 15rem;" src="logo.png"
      alt="User-Profile">
    <hr><br>
    <div class="card-body">
      <ul class="list-group list-group-flush">
        <li class="list-group-item"><strong>Username:</strong><%= info.firstName %></li>
        <li class="list-group-item"><strong>Course Type: </strong><%= info.courseType %></li>
        <li class="list-group-item"><strong>Gender: </strong><%= info.gender %></li>
        <li class="list-group-item"><strong>DOB: </strong><%= info.dateOfBirth %></li>
        <li class="list-group-item"><strong>E-mail: </strong><%= info.emailAddress %></li>
        <li class="list-group-item"><strong>Mobile Number: </strong><%= info.phoneNumber %></li>
        <li class="list-group-item"><strong>Area Pin Code: </strong><%= info.pinCode %></li>
        <li class="list-group-item"><strong>District: </strong><%= info.district %></li>
        <li class="list-group-item"><strong>State: </strong><%= info.state %></li>
        <li class="list-group-item"><strong>Address: </strong><%= info.address %></li>
        <li class="list-group-item"><strong>Final Grade: </strong><%= info.finalGrade %></li>
        <li class="list-group-item"><strong>Marksheet: </strong>

          <div class="card">
            <img class="card-img-top" src="uploads/<%=info.tenthMarksheet%>" alt="tenthMarsheet">
            <div class="card-body contactbutton">
              <a href="uploads/<%=info.tenthMarksheet%>" target="_blank" class="btn btn-primary contactbutton">View Image</a>
            </div>
          </div>

        </li>
      </ul>
    </div>
  </div>
</section>

</div>

<a href="#" class="to-top">
  <i class="fa fa-chevron-up"></i>
</a>

<%- include('partials/footer') %>